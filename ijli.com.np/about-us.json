                "keyComponents": {
                    "heading": {
                        "css": {
                            "color": "text-2xl py-2 text-blue-700",
                            "fontSize": "12px"
                        },
                        "content": "Innovative Japanese Language Institute",
                        "inlineCss": ""
                    },
                    "description": {
