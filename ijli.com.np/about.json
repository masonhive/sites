                "keyComponents": {
                    "heading": {
                        "css": {
                            "color": " font-size-5 mbr-pt-1",
                            "fontSize": "12px"
                        },
                        "content": "Innovative Japanese Language Institute",
                        "inlineCss": "{\"color\":\"#013ebd\"}"
                    },
                    "description": {
                        "content": "",
